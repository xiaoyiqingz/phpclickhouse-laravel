{
  "name": "xiaoyiqingz/phpclickhouse-laravel",
  "description": "Adapter of the most popular library https://github.com/smi2/phpClickHouse to Laravel",
  "keywords": [
    "php",
    "laravel",
    "clickhouse"
  ],
  "homepage": "https://github.com/glushkovds/phpClickHouse-laravel",
  "type": "library",
  "license": "MIT",
  "authors": [
    {
      "name": "Denis Glushkov",
      "email": "amkarovec@gmail.com",
      "homepage": "https://github.com/glushkovds"
    }
  ],
  "require": {
    "php": ">=7.1.0",
    "laravel/framework": "^6.0|^7.0|^8.0",
    "smi2/phpclickhouse": "1.5.3",
    "the-tinderbox/clickhouse-builder": "3.0.0|4.0.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^9.5",
    "orchestra/testbench": "4.x"
  },
  "suggest": {
  },
  "autoload": {
    "psr-4": {
      "PhpClickHouseLaravel\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
        "PhpClickHouseLaravel\\Tests\\": "tests/"
    }
  }
}
